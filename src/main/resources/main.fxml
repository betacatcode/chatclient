      <font>
        <Font size="18.0" />
      </font>
    </Label>
    <TextArea fx:id="sendText" layoutX="46.0" layoutY="291.0" onKeyPressed="#sendMsgByEnter" onKeyReleased="#cursorReturn" prefHeight="52.0" prefWidth="517.0">
